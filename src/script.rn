pub fn start() {
    let data = new_data();
    data.target_fps = 60;
    return data;
}

pub fn update(data) {
    data.window_size = (data.window_size.0 + 1, data.window_size.1 + 1);
    if data.window_size.0 > 1200 || data.window_size.1 > 800 {
        data.window_size = (0, 0);
    }
    data.target_fps = data.target_fps + 1;
    if data.target_fps > 2025 {
        data.exit = true;
    }
    return data;
}
